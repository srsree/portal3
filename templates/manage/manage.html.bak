                                        <th>Name</th>
                                        <th>Summary</th>
                                        <th>Created By</th>
                                        <th>Assigned To</th>
                                        <th>Status</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    
                                    {% for i in article_list %}
                                <tr>
                                    <td class="center">{{ i.id }}</td>
                                    <td>{{ i.name }}</td>
                                    <td>{{ i.summary|safe }}</td>
                                    <td class="center">{{ i.created_by.first_name }} {{ i.created_by.last_name }}</td>
                                    <td class="center">{{ i.assigned_to.first_name }} {{ i.assigned_to.last_name }}</td>
                                    <td class="center">{{ i.status }}</td>
                                    
                                    <td>
                                        {% get_has_permission request i "edit" as edit_perm %}
                                        {% get_has_permission request i "delete" as delete_perm %}
                                        {% get_has_permission request i "move" as move_perm %}
                                        {% get_has_permission request i "view" as view_perm %}
                                        {% get_has_permission request i "revert" as revert_perm %}
                                        {% get_hash_value i.id as hashval %} 
                                    {% if i.assigned_to == request.user or request.user.is_superuser %}
                                        {% if edit_perm %}
                                            
                                            <a class="btn btn-primary" href="/manage-section/edit-wflowarticle/{{hashval}}/">
                                                <i class="glyphicon glyphicon-edit">Edit </i>
                                            </a>
                                        {% endif %}
                                        <!--{% if delete_perm %}
                                            <a class="btn btn-danger" href="/edit-article/{{i.id}}/">
                                                <i class="glyphicon glyphicon-remove">Delete</i>
                                            </a>
                                        {% endif %}
                                        -->
                                        {% if not request.user.is_superuser %}
                                        {% if revert_perm %}
                                            <a class="btn btn-warning" href="/manage-section/revert-wflowarticle/{{hashval}}/">
                                                <i class="glyphicon glyphicon-hand-left">Revert</i>
                                            </a>
                                        {% endif %}
                                        {% endif %}
                                        
                                        {% if not request.user.is_superuser %}
                                        {% if move_perm %}
                                            {% if i.status == "Move to publish" %}
                                                <a class="btn btn-info" href="/manage-section/publish-wflowarticle/{{hashval}}/">
                                                    <i class="glyphicon glyphicon-ok-sign">Publish</i>
                                                </a>
                                            {% else %}
                                                <a class="btn btn-info" href="/manage-section/move-wflowarticle/{{hashval}}/">
                                                    <i class="glyphicon glyphicon-hand-right">Move</i>
                                                </a>
                                            {% endif %}
                                        {% endif %}
                                        {% endif %}
                                    {% endif %}
                                        {% if request.user.is_superuser %}
                                            <a class="btn btn-warning" href="/manage-section/assign-wflowarticle/{{hashval}}/">
                                                <i class="glyphicon glyphicon-share-alt">Assign</i>
                                            </a>
                                            {% if not i.status == "Published" %}
                                            <a class="btn btn-info" href="/manage-section/publish-wflowarticle/{{hashval}}/">
                                                    <i class="glyphicon glyphicon-ok-sign">Publish</i>
                                            </a>
                                            {% endif %}
                                        {% endif %}
                                        
                                        {% if view_perm %}
                                            <a class="btn btn-success" href="/manage-section/view-wflowarticle/{{hashval}}/">
                                                <i class="glyphicon glyphicon-screenshot">View</i>
                                            </a>
                                        {% endif %}
                                    </td>
                                </tr>
                                {% endfor %}
                                </tbody>
                            </table>
                        
                        {% else %}
                            <div class="alert alert-info">
                                No Records found.
                            </div>
                        {% endif %}
                    </div>
                </div>
            </div>
        </div>
    {% endif %}

