{% extends "manage/base.html" %}
{% load wsf_tags %}
{% load template_tags %}
{% block title %}{% if key == 'problems' %}Assessment{% elif key == 'assesment'%}Topic{% else %}{{key|capfirst}}{% endif %}{% endblock %}

{% block rightcontent %}

<!--################################## Article manage #############################-->
    {% if key == 'article' %}
    <div id="content" class="col-lg-10 col-sm-10" style="width:100%">
        <!-- content starts -->
        <div>
            <ul class="breadcrumb">
                <li>
                    <a href="/home/">Home</a>
                </li>
                <li>Article</li>
            </ul>
        </div>

        <div class="row">
            <div class="box col-md-12">
                <div class="box-inner">
                    <div class="box-header well" data-original-title="">
                        <h2><i class="glyphicon"></i>Article</h2>

                        <div class="box-icon">
                            <a href="/manage-section/add-article/" class="btn btn-round btn-default">
                                <i class="glyphicon glyphicon-plus-sign" title="Add Article">Add Article</i>
                            </a>
                            <a href="#" class="btn btn-minimize btn-round btn-default"><i
                                    class="glyphicon glyphicon-chevron-up"></i></a>
                        </div>
                    </div>
                    <div class="box-content">
                        {% if item_list %}
                            <table class="table table-striped table-bordered bootstrap-datatable datatable responsive">
                                <thead>
                                    <tr>
                                        <th>Headline</th>
                                        <th>Description</th>
                                        <th>Translated In</th>
                                        <th>Status</th>
                                        <th>Actions</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    {% for obj in item_list %}
                                        <tr>
                                            <td>{{ obj.headline }}</td>
                                            <td class="center">{{ obj.descriptor|safe|truncatewords:"10" }}</td>
                                            <td>{% if obj.get_translated_languages %}
                                                    {% for i in obj.get_translated_languages %}
                                                        {{i}}{% if not forloop.last %} , {% endif %}
                                                    {% endfor %}
                                                {% endif %}
                                            </td>
                                            <td class="center">
                                                {% if obj.active %}
                                                    <span class="label-success label label-default">Active</span>
                                                {% else %}
                                                    <span class="label-default label">Inactive</span>
                                                {% endif %}
                                            </td>
                                            <td class="center">
                                                <a class="btn btn-info" href="/manage-section/edit-article/?art_id={{obj.id}}">
                                                    <i class="glyphicon glyphicon-edit icon-white"></i>
                                                    Edit
                                                </a>
                                                {% if obj.active %}
                                                <a class="btn btn-danger" href="/manage-section/delete-article/?id={{obj.id}}&next={{request.get_full_path|urlencode}}">
                                                    <i class="glyphicon glyphicon-remove icon-white"></i>
                                                    Deactivate
                                                </a>
                                                {% else %}
                                                <a class="btn btn-success" href="/manage-section/activate-article/?id={{obj.id}}&next={{request.get_full_path|urlencode}}">
                                                    <i class="glyphicon glyphicon-ok icon-white"></i>
                                                    Activate
                                                </a>
                                                {% endif %}
                                                <a class="btn btn-warning" href="/manage/?key=manage-sections&art_id={{obj.id}}">
                                                    <i class="glyphicon glyphicon-th icon-white"></i>
                                                    Manage
                                                </a>
                                                <div class="btn-group">
                                                    <button class="btn btn-default btn-sm">Translate</button>
                                                    <button class="btn dropdown-toggle btn-default btn-sm" data-toggle="dropdown"><span class="caret"></span></button>
                                                    <ul class="dropdown-menu">
                                                    {% if obj.get_translated_languages %}
                                                        {% if "kannada" in obj.get_translated_languages %}
                                                        {% else %}
                                                            <li><a href="/manage-section/add-article/?atype={{atype}}&lang=kannada&ref={{obj.slug}}">Kannada</a></li>
                                                        {% endif %}
                                                    {% else %}
                                                    <li><a href="/manage-section/add-article/?atype={{atype}}&lang=kannada&ref={{obj.slug}}">Kannada</a></li>
                                                    {% endif %}
                                                    
                                                    </ul>
                                                </div>
                                            </td>
                                        </tr>
                                    {% endfor %}
                                </tbody>
                            </table>
                        
                        {% else %}
                            <div class="alert alert-info">
                                No Records found.
                            </div>
                        {% endif %}
                    </div>
                </div>
            </div>
        </div>
    {% endif %}
    
